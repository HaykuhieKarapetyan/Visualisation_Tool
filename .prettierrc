{
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "importOrder": [
        "^react",
        "^(?!(react|@core/))\\w+",
        "^(@reduxjs/)",
        "^antd",
        "^([@|(\\w)])",
        "",
        "^@(core|ui)/",
        "^."
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": false,
    "importOrderCaseInsensitive": true
}